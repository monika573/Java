        var changeLinkColor = function () {
            document.getElementById("ad-feedback-text-desktop-ad-center-1").style.color = '#111111';
            document.getElementById("ad-feedback-sprite-desktop-ad-center-1").style.background = 'transparent url("https://m.media-amazon.com/images/G/01/ad-feedback/New-info-icon-sprite-1x.png") no-repeat 0px -12px';
        }
        var onClick = function() {
             P.when('A').execute( function(A) {
                 A.on( 'a:popover:beforeShow', function() {
                     if(window.uet) {
                         window.uet('bb', 'adFeedback:desktkop:feedbackForm:time', {wb: 1});
                     }
                 });
                 A.on( 'a:popover:ajaxSuccess', function() {
                     if(window.ue && window.ue.count) {
                         window.ue.count("adFeedback:desktop:feedbackForm:success", 1);
                     }
                     if(window.uet) {
                         window.uet('be', 'adFeedback:desktkop:feedbackForm:time', {wb: 1});
                     }
                     if(window.uex) {
                         window.uex('ld', 'adFeedback:desktkop:feedbackForm:time', {wb: 1});
                     }
                 });
                 A.on( 'a:popover:ajaxFail', function() {
                     if(window.ue && window.ue.count) {
                         window.ue.count("adFeedback:desktop:feedbackForm:failure", 1);
                     }
                     if(window.uet) {
                         window.uet('be', 'adFeedback:desktkop:feedbackForm:time', {wb: 1});
                     }
                     if(window.uex) {
                         window.uex('ld', 'adFeedback:desktkop:feedbackForm:time', {wb: 1});
                     }
                 });
             });
        }
        var defaultLinkBehaviour = function() {
            document.getElementById("ad-feedback-text-desktop-ad-center-1").style.textDecoration = 'none';
            document.getElementById("ad-feedback-text-desktop-ad-center-1").style.color = '#555555';
            document.getElementById("ad-feedback-sprite-desktop-ad-center-1").style.background = 'transparent url("https://m.media-amazon.com/images/G/01/ad-feedback/New-info-icon-sprite-1x.png") no-repeat scroll 0px 0px';
        }
        var addFocusStyling = function () {
            document.getElementById("af-label-primary-link-desktop-ad-center-1").style.outline = '3px solid #35798a';
            document.getElementById("af-label-primary-link-desktop-ad-center-1").style.borderRadius = '5px';
        }
        var removeFocusStyling = function () {
            document.getElementById("af-label-primary-link-desktop-ad-center-1").style.outline = 'none';
            document.getElementById("af-label-primary-link-desktop-ad-center-1").style.borderRadius = '0';
        }
window.addFocusStyling349cc398b9cc4bbdba528da65e75cf91 = addFocusStyling;
window.removeFocusStyling349cc398b9cc4bbdba528da65e75cf91 = removeFocusStyling;
window.changeLinkColor349cc398b9cc4bbdba528da65e75cf91 = changeLinkColor;
window.defaultLinkBehaviour349cc398b9cc4bbdba528da65e75cf91 = defaultLinkBehaviour;
window.onClick349cc398b9cc4bbdba528da65e75cf91 = onClick;
    } catch(ex) {
        console.log("Error in link-rendering-js desktop") 
        if(window.ueLogError) {
            var additionalInfo = {
                logLevel : 'FATAL',
                attribution : 'Ad Feedback Desktop',
                message : 'Error in Feedback js desktop '
            };
            window.ueLogError(ex, additionalInfo);
        }
    }
})(window, document, Math);</script><a data-a-modal="{&quot;url&quot;:&quot;/af/feedback-form?pl=%7B%22adPlacementMetaData%22%3A%7B%22adElementId%22%3A%22ape_Gateway_desktop-ad-center-1_desktop_placement%22%2C%22pageType%22%3A%22Gateway%22%2C%22pageUrl%22%3A%%3D%22%2C%22slotName%22%3A%22desktop-ad-center-1%22%7D%2C%22adCreativeMetaData%22%3A%7B%22adProgramId%22%3A%221001%22%2C%22adCreativeTemplateName%22%3A%22Image%22%2C%22adImpressionId%22%3A%22https%3A%2F%2Faax-eu.amazon.in%2Fe%2Fxsp%2Fimp%3Fb%3DRAGq37xLqGp9xSxMced6ukkAAAGT_AnVRwMAAAH_AQBvbm9fdHhuX2JpZDEgICBvbm9fdHhuX2ltcDEgICAximm9%22%2C%22adCreativeId%22%3A%22577734139129646485%22%2C%22adId%22%3A%22581317124664072230%22%2C%22adCreativeDetails%22%3A%5B%5D%2C%22adNetwork%22%3A%22cs%22%7D%7D&amp;daFlg=false&amp;ie=UTF-8&quot;,   &quot;closeButtonLabel&quot;:&quot;Close Window&quot;,  &quot;popoverLabel&quot;:&quot;Advertisement feedback&quot;,  &quot;name&quot;:&quot;shared-placement-feedback-modal-desktop-ad-center-1&quot;,&quot;header&quot;:&quot;Leave feedback&quot;,&quot;width&quot;:&quot;460&quot;}" style="font-size: 11px; color: rgb(85, 85, 85); cursor: pointer; text-decoration: none; padding: 2px;" data-action="a-modal" class="a-declarative" tabindex="0" role="button" id="af-label-primary-link-desktop-ad-center-1" onfocus="window.addFocusStyling349cc398b9cc4bbdba528da65e75cf91()" onblur="window.removeFocusStyling349cc398b9cc4bbdba528da65e75cf91()">
    <span style="text-decoration: none;" id="ad-feedback-text-desktop-ad-center-1" onmouseover="window.changeLinkColor349cc398b9cc4bbdba528da65e75cf91()" onmouseout="window.defaultLinkBehaviour349cc398b9cc4bbdba528da65e75cf91()" onclick="window.onClick349cc398b9cc4bbdba528da65e75cf91()" aria-hidden="false" aria-label="Leave feedback on Sponsored advertisement"> Sponsored
